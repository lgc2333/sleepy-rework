{
  "name": "sleepy-rework-types",
  "version": "0.1.0",
  "packageManager": "yarn@4.9.1",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "scripts": {
    "generate": "uv run ../../scripts/gen_openapi_spec.py && openapi-typescript openapi.json -o src/openapi.ts && uv run ../../scripts/gen_js_api_type_anno.py && yarn format",
    "build:src": "tsc -b && dumble",
    "build": "yarn generate && yarn build:src",
    "clean": "rimraf dist && rimraf tsconfig.tsbuildinfo",
    "format": "prettier --ignore-path -cw src"
  },
  "devDependencies": {
    "dumble": "^0.2.2",
    "openapi-typescript": "^7.8.0",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.3"
  }
}
